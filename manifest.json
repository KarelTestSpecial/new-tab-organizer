{
  "manifest_version": 3,
  "name": "New Tab Organizer",
  "version": "1.1",
  "description": "A customizable startpage with bookmarks and notes.",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "startpage.html"
  },
  "action": {
    "default_icon": {
      "16": "assets/icon.png",
      "48": "assets/icon.png",
      "128": "assets/icon.png"
    }
  },
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  }
}
